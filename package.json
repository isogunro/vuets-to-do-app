{
  "name": "my-todo-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test:unit": "vitest"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/jest": "^29.1.1",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/vue3-jest": "^29.1.1",
    "happy-dom": "^6.0.4",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vitest": "^0.23.4",
    "vue-tsc": "^0.40.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest",
      ".*\\.(vue)$": "@vue/vue3-jest"
    },
    "testURL": "http://localhost/",
    "testEnvironment": "jsdom"
  },
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/src/$1"
  },
  "snapshotSerializers": [
    "jest-serializer-vue"
  ]
}